{
  "name": "fullkernelpanic/http-api-client",
  "description": "Guzzle based HTTP API client library",
  "type": "library",
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "HttpApiClient\\": "src/",
      "HttpApiClient\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "FullKernelPanic",
      "email": "zackraven@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/FullKernelPanic/api-client-base.git"
    }
  ],
  "require": {
    "php": ">=8.0",
    "fullkernelpanic/api-client-base": "^1.0.0",
    "guzzlehttp/guzzle": "^7.3",
    "psr/log": " ^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0.0"
  }
}
